@{ 
    Layout = null;
}

<!DOCTYPE html>
<html class="no-js ss-preload" lang="en">
@Html.Action("PartialHead")

<body id="top">


    <!-- # preloader
    ================================================== -->
    <div id="preloader">
        <div id="loader">
        </div>
    </div>


    <!-- # page wrap
    ================================================== -->
    <div class="s-pagewrap">

        <div class="circles">
            <span></span>
            <span></span>
            <span></span>
            <span></span>
            <span></span>
        </div>
        
    @Html.Action("PartialSiteHeader")
        <!-- ## main content
        ==================================================- -->
        <main class="s-content">


            <!-- ### intro
            ================================================== -->
        @Html.Action("PartialIntro")
             <!-- end s-intro -->
            <!-- ### about
            ================================================== -->
            <section id="about" class="s-about target-section">


               @Html.Action("PartialAbout")

                   <!-- about-info -->


                <div class="row about-expertise" data-animate-block>
                    <div class="column lg-12">

                        <h2 class="text-pretitle" data-animate-el>Expertise</h2>

                        <ul class="skills-list h1" data-animate-el>
                            <li>Visual Design</li>
                            <li>Branding Identity</li>
                            <li>UI Design</li>
                            <li>Product Design</li>
                            <li>Prototyping</li>
                            <li>Illustration</li>
                        </ul>

                    </div>
                </div> <!-- end about-expertise -->


                <div class="row about-timelines" data-animate-block>

                   @Html.Action("PartialExperience")
                @Html.Action("PartialEducation")

                         <!-- end timeline -->

                    </div> <!-- end column -->

                    <!-- end timeline -->

                    </div> <!-- end column -->


                </div> <!-- end about-timelines -->

            </section> <!-- end s-about -->
            <!-- ### works
            ================================================== -->
            <section id="works" class="s-works target-section">
            @Html.Action("PartialProject")

               <!-- end works-portfolio -->


            @Html.Action("PartialTestimonial")            
    <!-- end row testimonials -->

            </section> <!-- end s-works -->
            <!-- ### contact
            ================================================== -->
            @Html.Action("PartialContact")       
    <!-- end contact -->

        </main> <!-- end s-content -->
        <!-- ## footer
        ================================================== -->
        @Html.Action("PartialFooter")
    <!-- end s-footer -->

    </div> <!-- end -s-pagewrap -->
    <!-- Java Script
    ================================================== -->
    @Html.Action("PartialScripts")

</body>
</html>