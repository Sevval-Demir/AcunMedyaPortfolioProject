@model AcunMedyaPortfolioProject.Models.Expertise
@{
    ViewBag.Title = "CreateExpertise";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yeni Uzmanlık Ekleme</h2>

<div class="container">
    <form method="post">
        <label>Uzmanlık İsmi</label>
        @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
        <br />
        <button class="btn btn-info">Yeni Uzmanlık Ekleme</button>
    </form>
</div>

