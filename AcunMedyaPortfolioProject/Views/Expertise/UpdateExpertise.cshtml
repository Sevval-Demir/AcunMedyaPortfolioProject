@model AcunMedyaPortfolioProject.Models.Expertise
@{
    ViewBag.Title = "UpdateExpertise";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Uzamanlık Güncelleme</h2>

<div class="container">
    <form method="post">
        @Html.HiddenFor(x => x.Id)
        <label>Başlık</label>
        @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
        <br />
        <button class="btn btn-info">Uzmanlık Güncelleme</button>
    </form>
</div>

